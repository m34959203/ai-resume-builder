{
  "name": "ai-resume-builder",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:all": "concurrently \"npm run dev\" \"npm run server:dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "start": "concurrently \"npm run server:start\" \"npm run preview\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "server:dev": "npm --prefix server run start:dev",
    "server:start": "npm --prefix server run start",
    "dev:auto": "node scripts/autoupdater.js"
  },
  "dependencies": {
    "@react-pdf/renderer": "^3.4.5",
    "lucide-react": "^0.447.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.15",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.21",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "jsdom": "^28.0.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.18",
    "vite": "^5.4.10",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.18"
  }
}