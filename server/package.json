{
  "name": "ai-resume-backend",
  "version": "2.0.0",
  "private": true,
  "description": "Backend-for-Frontend для AI Resume Builder: HH API proxy, AI translation (Gemini), AI analysis (DeepSeek), OAuth",
  "main": "index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch . --ext js,json --signal SIGTERM index.js",
    "start:dev": "npm run dev",
    "start:inspect": "cross-env NODE_ENV=development node --inspect=0.0.0.0:9229 index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.2"
  },
  "license": "MIT",
  "keywords": [
    "express",
    "bff",
    "backend-for-frontend",
    "headhunter",
    "hh-api",
    "ai",
    "gemini",
    "deepseek",
    "translation",
    "resume",
    "openrouter"
  ]
}