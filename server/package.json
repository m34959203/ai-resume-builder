{
  "name": "resume-bff",
  "version": "2.0.0",
  "private": true,
  "description": "Backend-for-Frontend (Express) для AI Resume Builder: прокси HH API, OAuth, AI переводы",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch . --ext js --signal SIGTERM index.js",
    "start:dev": "npm run dev",
    "start:inspect": "cross-env NODE_ENV=development node --inspect=0.0.0.0:9229 index.js",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "dependencies": {
    "express": "^4.19.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express-rate-limit": "^7.3.1",
    "node-cache": "^5.1.2"
  },
  "optionalDependencies": {
    "compression": "^1.7.4",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.2"
  },
  "license": "UNLICENSED",
  "keywords": [
    "express",
    "bff",
    "proxy",
    "hh",
    "ai",
    "translation",
    "resume",
    "i18n"
  ]
}